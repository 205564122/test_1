#include<stdio.h>
int main()
{
	int arr[3][4]={{1,2},{3,4,5}};                   //二维数组可以省略行，但不能省略列
	int i;
	int j;
	for(i=0;i<3;i++)
	{
		for(j=0;j<4;j++)
		{
			printf("%d\t",arr[i][j]);
		}
		printf("\n");
	}
	for(i=0;i<3;i++)
	{
		for(j=0;j<4;j++)
		{
			printf("%p\t",&arr[i][j]);
		}
		printf("\n");
	}
	return 0;
}
